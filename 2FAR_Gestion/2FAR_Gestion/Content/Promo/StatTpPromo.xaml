<Page x:Class="_2FAR_Gestion.Content.Promo.StatTpPromo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
      xmlns:promo="clr-namespace:_2FAR_Gestion.Content.Promo"
      mc:Ignorable="d"
      Title="StatTpPromo" Height="450" Width="800">
    <Page.DataContext>
        <promo:StatTpPromo/>
    </Page.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <wpf:CartesianChart Series="{Binding Series}"></wpf:CartesianChart>
    </Grid>
</Page>